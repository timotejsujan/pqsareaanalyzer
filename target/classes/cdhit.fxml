        <font>
            <Font size="13.0" />
        </font>
    </Button>
    <TextField fx:id="output_dir" editable="false" layoutX="49.0" layoutY="179.0" onMouseClicked="#set_output_dir" styleClass="input" text="Select output directory..." />
