        <font>
            <Font size="13.0" />
        </font>
    </Button>
    <Label layoutX="18.0" layoutY="238.0" text="Name" />
    <TextField fx:id="output_name" layoutX="100.0" layoutY="233.0" prefHeight="27.0" prefWidth="100.0" />
    <Label layoutX="18.0" layoutY="280.0" text="Parameters" />
    <TextField fx:id="params" layoutX="100.0" layoutY="275.0" prefHeight="27.0" prefWidth="674.0" />
