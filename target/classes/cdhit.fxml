        <font>
            <Font size="13.0" />
        </font>
    </Button>
    <TextField fx:id="input_path_pqs" editable="false" layoutX="49.0" layoutY="139.0" onMouseClicked="#set_input_path_pqs" styleClass="input" text="Select PQS positions (pqsfinder output)..." />
