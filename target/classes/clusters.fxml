            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
            <children>

            </children>
        </VBox>
    </ScrollPane>
    <Label layoutX="47.0" layoutY="88.0" style="-fx-text-fill: red" text="*">*</Label>

</Pane>
